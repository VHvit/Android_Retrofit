                    android:textSize="30dp"/>

                <TextView
                    android:id="@+id/query_2_2"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_marginStart="50dp"
                    android:layout_marginTop="10dp"
